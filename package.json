{
  "name": "mohit-kumar",
  "version": "2.5.3",
  "description": "Mohit Kumar portfolio!",
  "main": "index.js",
  "scripts": {
    "get-about": "ts-node --files src/scripts/get-about",
    "check-types": "tsc -p . --noEmit",
    "prebuild": "npm run get-about && npm run check-types",
    "build": "gatsby build",
    "prestart": "npm run get-about",
    "start": "gatsby develop",
    "clean": "rm -rf node_modules && gatsby clean",
    "serve": "npm run build && gatsby serve -p 9000",
    "lint": "eslint ./src",
    "test": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress run",
    "test:open": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress open",
    "test:ci": "DETERMINISTIC=true start-server-and-test 8000",
    "update-media": "cypress run -C ./cypress-media.json",
    "update-media:ci": "DETERMINISTIC=true start-server-and-test 8000 update-media",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajaykumar97/ajay-kumar"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Mohit Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajaykumar97/ajay-kumar/issues"
  },
  "homepage": "https://github.com/ajaykumar97/ajay-kumar#readme",
