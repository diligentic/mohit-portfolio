    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-launchdarkly": "^0.3.2",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
